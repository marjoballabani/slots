div.row.main-container
    div.col-md-3.events-container.animated.fadeInLeft
        h3
            | Logs
            hr
        div(
            ng-repeat="event in $ctrl.history track by $index"
        )
            label.status not avaliable
            label.id
                |  {{event}}

    div.col-md-8.calendar-container.col-md-push-1
        div(
            ui-calendar="$ctrl.uiConfig.calendar"
            ng-model="$ctrl.events",
            calendar="calendar"
        )